<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200">
      <children>
         <VBox alignment="CENTER" layoutX="50.0" prefHeight="200.0" prefWidth="100.0">
            <children>
               <Label text="Choose File:" />
               <ComboBox fx:id="combo" prefWidth="150.0" promptText="Open" />
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
