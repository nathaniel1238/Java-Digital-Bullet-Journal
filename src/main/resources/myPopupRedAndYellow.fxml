<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200">
      <children>
         <VBox alignment="CENTER" prefHeight="398.0" prefWidth="486.0" style="-fx-background-color: #F9E795; -fx-border-color: #F96167;">
            <children>
               <Label fx:id="popupLabel" alignment="CENTER" prefHeight="177.0" prefWidth="201.0" text="Contents">
                  <font>
                     <Font name="Tsukushi A Round Gothic Regular" size="15.0" />
                  </font>
               </Label>
               <Button fx:id="close" mnemonicParsing="false" text="Close" />
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
