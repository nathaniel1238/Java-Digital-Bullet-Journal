<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <root>
      <AnchorPane prefHeight="268.0" prefWidth="332.0">
         <children>
            <VBox alignment="CENTER" prefHeight="398.0" prefWidth="486.0" style="-fx-background-color: #2F3C7E; -fx-border-color: #FBEAEB;">
               <children>
                  <Label fx:id="popupLabel" alignment="CENTER" prefHeight="219.0" prefWidth="266.0" text="Contents" textFill="WHITE">
                     <font>
                        <Font name="Times New Roman Bold Italic" size="15.0" />
                     </font>
                  </Label>
                  <Button fx:id="close" mnemonicParsing="false" text="Close" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </root>
</Scene>
